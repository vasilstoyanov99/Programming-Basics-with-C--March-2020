using System;

namespace _05.ProjectsCreation
{
    class Program
    {
        static void Main(string[] args)
        {
            string name = Console.ReadLine();
            int count = int.Parse(Console.ReadLine());
            var total = count * 3;

            Console.WriteLine($"The architect {name} will need {total} hours to complete {count} project/s.");
        }
    }
}
